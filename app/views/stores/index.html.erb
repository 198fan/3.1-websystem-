<h1>店舗データ</h1>

<table class="table table-hover table-striped table-bordered">
  <thead class="table-dark">
    <th>店舗ID</th>
    <th>店舗名</th>
    <th>メールアドレス</th>
    <th>住所</th>
    <th>詳細</th>
  </thead>

  <tbody>
    <div id="stores">
      <% @stores.each do |store| %>
      <%= render store %>
      <% end %>
    </div>
  </tbody>
</table>
