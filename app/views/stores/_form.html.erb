<%= form_with(model: store) do |form| %>
  <% if store.errors.any? %>
    <div style="color: red">
      <h2><%= pluralize(store.errors.count, "error") %> prohibited this store from being saved:</h2>

      <ul>
        <% store.errors.each do |error| %>
          <li><%= error.full_message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div  class="mb-3">
    <%= form.number_field :storeid , class:"form-control", placeholder:"店舗ID" %>
  </div>

  <div  class="mb-3">
    <%= form.text_field :storename , class:"form-control", placeholder:"店舗名"%>
  </div>

  <div  class="mb-3">

    <%= form.text_field :mail , class:"form-control", placeholder:"メールアドレス" %>
  </div>

  <div  class="mb-3">

    <%= form.text_area :address , class:"form-control", placeholder:"住所" %>
  </div>

  <div>
    <%= form.submit "登録" ,class:"btn btn-dark"%>
  </div>
<% end %>
